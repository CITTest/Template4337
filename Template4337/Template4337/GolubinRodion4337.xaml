<Window x:Class="Template4337.GolubinRodion4337"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Template4337"
        mc:Ignorable="d"
        Title="GolubinRodion4337" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label Grid.Column="0" Grid.Row="0" Content="Голубин Родион 4337"></Label>
        <Button Grid.Column="0" Grid.Row="1" Click="ExportExcel">Экспорт excel</Button>
        <Button Grid.Column="0" Grid.Row="2" Click="ImportExcel">Импорт данных excel</Button>
        <Button Grid.Column="1" Grid.Row="1" Click="ImportJson">Импорт JSON</Button>
        <Button Grid.Column="1" Grid.Row="2" Click="ExportWord">экспорт Word</Button>
    </Grid>
    
</Window>
